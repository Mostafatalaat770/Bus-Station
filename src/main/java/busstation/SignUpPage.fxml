<AnchorPane prefHeight="500.0" prefWidth="1000.0" style="-fx-background-color: #669999;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="busstation.Controller">
   <children>
      <AnchorPane prefHeight="50.0" prefWidth="1000.0" style="-fx-background-color: #226666;">
         <children>
            <Text layoutX="14.0" layoutY="31.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome new user">
               <font>
                  <Font name="Calibri Bold" size="21.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
      <Text layoutX="14.0" layoutY="92.0" strokeType="OUTSIDE" strokeWidth="0.0" text="how do you want us to call you?" />
      <JFXTextField fx:id="signUpFirstNameTextField" layoutX="14.0" layoutY="103.0" maxWidth="213.0" minWidth="213.0" prefHeight="37.0" prefWidth="213.0" promptText="First name" />
      <JFXTextField fx:id="signUpLastNameTextField" layoutX="14.0" layoutY="149.0" maxWidth="213.0" minWidth="213.0" prefHeight="37.0" prefWidth="213.0" promptText="Last name" />
      <Text layoutX="14.0" layoutY="224.0" strokeType="OUTSIDE" strokeWidth="0.0" text="How old are you?" wrappingWidth="213.0" />
      <JFXTextField fx:id="signUpAgeTextField" layoutX="14.0" layoutY="242.0" maxWidth="213.0" minWidth="213.0" prefHeight="37.0" prefWidth="213.0" promptText="Age" />
      <Text layoutX="14.0" layoutY="317.0" strokeType="OUTSIDE" strokeWidth="0.0" text="What about your username?" />
